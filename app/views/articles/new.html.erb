<h2>New Article</h2>
<%= form_for(@article, url: create_article_path, :method => :post) do |f| %>
  <p> 
	<%= f.label :title %><br/>
	<%= f.text_field :title %>
  </p>

  <p> 
	<%= f.label :content %><br/>
	<%= f.text_area :content %>
  </p>

  <p> 
	<%= f.submit %>
  </p>

<% end %>
<%= link_to 'Back', user_articles_path %>
